<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintNotis"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment_usuario">

    <TextView
        android:id="@+id/lbPerfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/Perfil"
        android:textColor="@color/Azul1"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/imgvPriv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgvPriv"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/foto_redonda"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/txtPrivacidadySeguridad"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtPrivacidadySeguridad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginBottom="46dp"
        android:fontFamily="@font/poppinsmedium"
        android:text="@string/duo"
        android:textColor="@color/Azul1"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imgvPerfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtNotiiii"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinsregular"
        android:text="@string/ejemploco"
        android:textColor="@color/Azul1"
        app:layout_constraintEnd_toEndOf="@+id/txtPrivacidadySeguridad"
        app:layout_constraintStart_toStartOf="@+id/txtPrivacidadySeguridad"
        app:layout_constraintTop_toBottomOf="@+id/txtPrivacidadySeguridad" />

    <ImageView
        android:id="@+id/imgvPerfil"
        android:layout_width="228dp"
        android:layout_height="50dp"
        android:layout_marginTop="268dp"
        android:src="@drawable/textboxprueba"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lbPersonalizarPerfil"
        android:layout_width="150dp"
        android:textAlignment="center"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/Personalizar"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvPerfil"
        app:layout_constraintEnd_toEndOf="@+id/imgvPerfil"
        app:layout_constraintStart_toStartOf="@+id/imgvPerfil" />

    <ImageButton
        android:id="@+id/imgvHistCitas"
        android:layout_width="308dp"
        android:layout_height="68dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/miscitas"
        android:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgvRecetas"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imgvDocFav"
        android:layout_width="308dp"
        android:layout_height="68dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/misdocsfavs"
        android:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/imgvRecetas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgvPerfil"
        app:layout_constraintVertical_bias="0.959" />

    <ImageButton
        android:id="@+id/imgvRecetas"
        android:layout_width="308dp"
        android:layout_height="68dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="188dp"
        android:background="@drawable/misrecetas"
        android:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imgvMisReseñas"
        android:layout_width="308dp"
        android:layout_height="68dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/misresenas"
        android:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgvHistCitas"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtMisDoctores"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/docsfav"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvDocFav"
        app:layout_constraintEnd_toEndOf="@+id/imgvDocFav"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/imgvDocFav"
        app:layout_constraintTop_toTopOf="@+id/imgvDocFav"
        app:layout_constraintVertical_bias="0.463" />

    <TextView
        android:id="@+id/txtMisRecetas"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/misrecetas"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvRecetas"
        app:layout_constraintEnd_toEndOf="@+id/imgvRecetas"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/imgvRecetas"
        app:layout_constraintTop_toTopOf="@+id/imgvRecetas"
        app:layout_constraintVertical_bias="0.487" />

    <TextView
        android:id="@+id/txtMisCitas"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/mihisto"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvHistCitas"
        app:layout_constraintEnd_toEndOf="@+id/imgvHistCitas"
        app:layout_constraintHorizontal_bias="0.768"
        app:layout_constraintStart_toStartOf="@+id/imgvHistCitas"
        app:layout_constraintTop_toTopOf="@+id/imgvHistCitas"
        app:layout_constraintVertical_bias="0.487" />

    <TextView
        android:id="@+id/txtMisResenas"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinssemibold"
        android:text="@string/misrese"
        android:textColor="@color/black"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgvMisReseñas"
        app:layout_constraintEnd_toEndOf="@+id/imgvMisReseñas"
        app:layout_constraintHorizontal_bias="0.759"
        app:layout_constraintStart_toStartOf="@+id/imgvMisReseñas"
        app:layout_constraintTop_toTopOf="@+id/imgvMisReseñas"
        app:layout_constraintVertical_bias="0.512" />


</androidx.constraintlayout.widget.ConstraintLayout>